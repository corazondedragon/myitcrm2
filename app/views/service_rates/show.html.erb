<div id="sidebar_left">
  <div class="module">
    <ul>
      <% if can? :read, @products %><li class="delete"><%= link_to (t "global.delete"), @service_rate, :confirm => 'Are you sure?', :method => :delete %></li> <% end %>
      <% if can? :edit, @products %><li class="edit"><%= link_to (t "global.edit"), edit_service_rate_path(@service_rate) %></li> <% end %>
    </ul>
  </div>
<%#= render 'layouts/left_sidebar_modules' %>
</div>
<div id="center_col" class="no_right_sidebar">

<div class="tabcontainer">

<div id="tab1" class="tabcontent">
  <p>
    <b>Sku:</b>
    <%= @service_rate.sku %>
  </p>

  <p>
    <b>Description:</b>
    <%= @service_rate.description %>
  </p>

  <p>
    <b>Rate:</b>
    <%= number_to_currency(@service_rate.rate) %>
  </p>

  <p>
    <b>Taxable:</b>
    <%= @service_rate.taxable %>
  </p>

  <p>
    <b>Tax rate:</b>
    <%= @service_rate.tax_rate %>
  </p>

  <p>
    <b>Active:</b>
    <%= @service_rate.active %>
  </p>

  <fieldset id="collapsable">
    <table>
      <tr>
        <td  >
          <h1><%= t "global.transaction_log" %></h1>
        </td>
      </tr>
      <tr>
        <td  >
          <strong><%= t "supplier.date_created" %>:</strong>
        </td>
        <td align="left">
          <%= time_ago_in_words(@service_rate.created_at) %> <%= t "global.ago" %>
          (<%= localize @service_rate.created_at, :format => :short %>).
        </td>
      </tr>
      <tr>
        <td >
          <strong><%= t "global.last_updated_by" %>:</strong>
        </td>
        <td align="left">
              <%= time_ago_in_words(@service_rate.created_at) %> <%= t "global.ago" %>
              (<%= localize @service_rate.created_at, :format => :short %>).

        </td>
      </tr>
    </table>
  </fieldset>
</div>
</div>
</div>
